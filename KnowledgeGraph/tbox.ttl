nco:Aspect a owl:Class ;
    rdfs:comment "The notion of entity aspect discussed here is related to the second level of agenda setting theory, which deals with the salience of the attributes of the entities that are the focus of attention in the media (McCombs et al., 2014)"@en .

nco:Entity a owl:Class .

nco:Justification a owl:Class ;
    rdfs:comment "Class Justification refers to the justification for a claim.  Because this ontology focuses on news classification rather than argumentation, the definition here is simply a placeholder and is not further elaborated"@en ;
    rdfs:subClassOf owl:Thing .

nco:Location a owl:Class ;
    rdfs:label "Location"@en ;
    rdfs:seeAlso "dul:Location" .

nco:Person a owl:Class ;
    rdfs:subClassOf nco:PersonOrOganization .

nco:Predicate a owl:Class ;
    rdfs:comment "This is a metaclass whose instances play the role of a predicate in reified triples.  It is needed because OWL does not allow object properties to be used directly as values in triples unless an individual with the same name has been defined"@en .

nco:concernsTopic a owl:ObjectProperty ;
    rdfs:label "concerns topic"@en ;
    rdfs:comment "A claim is something that concerns a topic.  The property concernsTopic is used to express this relation" ;
    rdfs:domain nco:Claim ;
    rdfs:range nco:Topic .

nco:hasAgent a owl:ObjectProperty ;
    rdfs:label "has agent"@en ;
    rdfs:domain nco:AgentComponent ;
    rdfs:range nco:Agent ;
    owl:inverseOf nco:isAgentOf .

nco:hasAgentRole a owl:ObjectProperty ;
    rdfs:label "has agent role"@en ;
    rdfs:domain nco:AgentComponent ;
    rdfs:range nco:AgentRole ;
    owl:inverseOf nco:isAgentRoleOf .

nco:hasClaim a owl:ObjectProperty ;
    rdfs:label "has claim"@en ;
    rdfs:comment "Captures the relation between a news item and a claim expressed by an agent, which is reported in the news item in question"@en ;
    rdfs:domain nco:NewsItem ;
    rdfs:range nco:Claim .

nco:hasNewsSource a owl:ObjectProperty ;
    rdfs:domain nco:NewsItem ;
    rdfs:range nco:NewsSource ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    owl:inverseOf nco:isNewsSourceOf .

nco:hasOmittedEvent a owl:ObjectProperty ;
    rdfs:label "has omitted event"@en ;
    rdfs:comment "captures the relation between an OmissionEvent and the event that has not been carried out - e.g., a soldier's failure to carry out an order"@en ;
    rdfs:domain nco:OmissionEvent ;
    rdfs:range nco:Event ;
    owl:inverseOf nco:isOmittedEventOf .

nco:hasPublisher a owl:ObjectProperty ;
    rdfs:domain nco:NewsSource ;
    rdfs:range nco:Publisher ;
    owl:inverseOf nco:isPublisherOf .

nco:hasTime a owl:ObjectProperty ;
    rdfs:label "has time"@en ;
    rdfs:domain nco:EventOrSituation ;
    rdfs:range time:TemporalEntity ;
    rdfs:subPropertyOf time:hasTime ;
    owl:inverseOf nco:isTimeOf ;
    owl:propertyChainAxiom ( nco:isOmittedEventOf nco:hasTime ) .

nco:hasUtterance a owl:ObjectProperty ;
    rdfs:domain nco:Claim ;
    rdfs:range nco:Utterance ;
    owl:inverseOf nco:isUtteranceOf .

nco:hasUtteranceType a owl:ObjectProperty ;
    owl:inverseOf nco:isUtteranceTypeOf .

nco:isAspectOf a owl:ObjectProperty ;
    rdfs:label "is aspect of"@en ;
    rdfs:domain nco:Aspect ;
    rdfs:range owl:Thing .

nco:isOmittedEventOf a owl:ObjectProperty .

nco:isTimeOf a owl:ObjectProperty .

nco:preconditionFor a owl:ObjectProperty,
        owl:TransitiveProperty ;
    rdfs:label "precondition for"@en ;
    rdfs:domain nco:EventOrSituation ;
    rdfs:range nco:EventOrSituation .

nco:preconditionForEvent a owl:ObjectProperty ;
    rdfs:label "precondition for event"@en ;
    rdfs:domain nco:EventOrSituation ;
    rdfs:range nco:Event ;
    rdfs:subPropertyOf nco:preconditionFor .

nco:subTopicOf a owl:ObjectProperty,
        owl:TransitiveProperty ;
    rdfs:label "subtopic of"@en ;
    rdfs:domain nco:Topic ;
    rdfs:range nco:Topic ;
    rdfs:subPropertyOf skos:broaderTransitive .

nco:ufSatisfiedBy a owl:ObjectProperty ;
    rdfs:label "satisfied by"@en .

schema:LegalService a owl:Class ;
    rdfs:label "LegalService"@en ;
    rdfs:comment "A LegalService is a business that provides legally-oriented services, advice and representation, e.g. law firms.\\n\\nAs a [[LocalBusiness]] it can be described as a [[provider]] of one or more [[Service]]\\(s)."@en ;
    rdfs:subClassOf schema:LocalBusiness .

schema:Physician a owl:Class ;
    rdfs:label "Physician"@en ;
    rdfs:comment "An individual physician or a physician's office considered as a [[MedicalOrganization]]."@en ;
    rdfs:subClassOf schema:MedicalBusiness,
        schema:MedicalOrganization .

schema:Project a owl:Class ;
    rdfs:label "Project"@en ;
    rdfs:comment """An enterprise (potentially individual but typically collaborative), planned to achieve a particular aim.\r
Use properties from [[Organization]], [[subOrganization]]/[[parentOrganization]] to indicate project sub-structures. \r
   """@en ;
    rdfs:subClassOf schema:Organization .

nco:AgentRole a owl:Class ;
    rdfs:label "Agent role"@en .

nco:Description a owl:Class ;
    rdfs:label "Description"@en ;
    rdfs:comment "Like a Situation, a Description is also characterised as a Collection of Statements"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom nco:Situation ;
            owl:onProperty nco:characterises ],
        [ a owl:Restriction ;
            owl:allValuesFrom nco:Statement ;
            owl:onProperty nco:hasElement ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass nco:Situation ;
            owl:onProperty nco:characterises ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass nco:Statement ;
            owl:onProperty nco:hasElement ],
        nco:Collection .

nco:Organization a owl:Class ;
    rdfs:subClassOf nco:PersonOrOganization ;
    owl:equivalentClass <http://schema.org/Organization> .

nco:PersonOrOganization a owl:Class ;
    rdfs:subClassOf nco:Agent ;
    owl:equivalentClass [ a owl:Class ;
            owl:unionOf ( nco:Organization nco:Person ) ] .

nco:characterises a owl:ObjectProperty ;
    rdfs:label "characterises"@en ;
    rdfs:comment "This is the main relation linking descriptions to situations. A description provides an interpretation of a situation."@en ;
    rdfs:domain nco:Description ;
    rdfs:range nco:Situation .

nco:hasAgentComponent a owl:ObjectProperty ;
    rdfs:label "has agent component"@en ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( nco:Claim nco:Event ) ] ;
    rdfs:range nco:AgentComponent .

nco:hasPrediction a owl:ObjectProperty ;
    rdfs:comment "captures the relation between a PredictionEvent and the event that has been predicted - forecasting an eruption"@en ;
    rdfs:domain nco:PredictionEvent ;
    rdfs:range nco:EventOrSituation .

nco:inNewsItem a owl:ObjectProperty ;
    rdfs:label "in news item"@en .

nco:isClaimOf a owl:ObjectProperty ;
    rdfs:label "is claim of"@en ;
    rdfs:subPropertyOf nco:isElementOf .

schema:EmergencyService a owl:Class ;
    rdfs:label "EmergencyService"@en ;
    rdfs:comment "An emergency service, such as a fire station or ER."@en ;
    rdfs:subClassOf schema:LocalBusiness .

schema:PerformingGroup a owl:Class ;
    rdfs:label "PerformingGroup"@en ;
    rdfs:comment "A performance group, such as a band, an orchestra, or a circus."@en ;
    rdfs:subClassOf schema:Organization .

nco:Publisher a owl:Class ;
    rdfs:comment "A publisher can be either a person or an organization" ;
    rdfs:subClassOf [ a owl:Class ;
            owl:unionOf ( nco:Organization nco:Person ) ],
        nco:PersonOrOganization .

nco:Utterance a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass nco:UtteranceType ;
            owl:onProperty nco:hasUtteranceType ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty nco:hasText ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty nco:hasTextContext ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] .

nco:Viewpoint a owl:Class ;
    rdfs:label "Viewpoint"@en ;
    rdfs:comment "As pointed out in (Baden and Springer, 2017), when analysing the news discourse, we are interested in identifying viewpoints that “open up different perspectives” and “construct different meaning”. Hence, here we define a Viewpoint as a collection of claims that subscribe to the same perspective"@en ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:allValuesFrom nco:Claim ;
                        owl:onProperty nco:hasElement ] [ a owl:Restriction ;
                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass nco:Claim ;
                        owl:onProperty nco:hasElement ] ) ],
        nco:Collection .

nco:hasUnifyingFactor a owl:ObjectProperty ;
    rdfs:label "has unifying factor"@en ;
    rdfs:comment "Relates a collection to the associated unifying factor"@en ;
    rdfs:domain nco:Collection ;
    rdfs:range nco:UnifyingFactor ;
    owl:inverseOf nco:isUnifyingFactorOf .

nco:isElementOf a owl:ObjectProperty ;
    rdfs:label "is element of"@en ;
    rdfs:domain owl:Thing ;
    rdfs:range nco:Collection .

schema:FinancialService a owl:Class ;
    rdfs:label "FinancialService"@en ;
    rdfs:comment "Financial services business."@en ;
    rdfs:subClassOf schema:LocalBusiness .

nco:Agent a owl:Class ;
    rdfs:label "Agent"@en ;
    rdfs:subClassOf nco:Entity .

nco:NewsSource a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass nco:Publisher ;
            owl:onProperty nco:hasPublisher ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] .

nco:Situation a owl:Class ;
    rdfs:label "Situation"@en ;
    rdfs:comment "A Situation is a collection of statements" ;
    rdfs:seeAlso "dul:Situation"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom nco:Description ;
            owl:onProperty nco:characterisedBy ],
        [ a owl:Restriction ;
            owl:allValuesFrom nco:Statement ;
            owl:onProperty nco:hasElement ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass nco:Statement ;
            owl:onProperty nco:hasElement ],
        nco:Collection,
        nco:EventOrSituation .

nco:UnifyingFactor a owl:Class ;
    rdfs:label "Unifying factor"@en .

nco:UtteranceType a owl:Class .

nco:topicRole a owl:FunctionalProperty,
        owl:InverseFunctionalProperty,
        owl:ObjectProperty ;
    rdfs:comment "It is essential to distinguish an entity, e.g., the individual J.F. Kennedy, from the associated topic, e.g., T_J.F. Kennedy.  These are two different ontological objects with very different properties. For instance, while the time span of  J.F. Kennedy the person was 1917-1963,  the time span of the associated topic continued after his death.  More in general, this relation is needed every time an individual in our knowledge base, which is not ontologically a topic (e.g., a person, an event, a statement, etc.) plays the role of a topic in the model."@en ;
    rdfs:domain owl:Thing ;
    rdfs:range nco:Topic ;
    owl:inverseOf nco:topicRoleOf .

nco:hasTopic a nco:Predicate,
        owl:NamedIndividual,
        owl:ObjectProperty ;
    rdfs:comment "captures the relation between a news item and a topic, which is the focus of the news item in question"@en ;
    rdfs:domain nco:NewsItem ;
    rdfs:range nco:Topic ;
    owl:inverseOf nco:topicInNewsItem ;
    owl:propertyChainAxiom ( nco:hasTopic nco:topicRoleOf nco:isAspectOf nco:topicRole ),
        ( nco:hasTopic nco:topicRoleOf nco:isClaimOf nco:topicRole ),
        ( nco:hasTopic nco:topicRoleOf nco:object nco:topicRole ),
        ( nco:hasTopic nco:topicRoleOf nco:predicate nco:topicRole ),
        ( nco:hasTopic nco:topicRoleOf nco:subject nco:topicRole ),
        ( nco:hasTopic nco:subTopicOf ) .

schema:EducationalOrganization a owl:Class ;
    rdfs:label "EducationalOrganization"@en ;
    rdfs:comment "An educational organization."@en ;
    rdfs:subClassOf schema:CivicStructure,
        schema:Organization .

schema:HealthAndBeautyBusiness a owl:Class ;
    rdfs:label "HealthAndBeautyBusiness"@en ;
    rdfs:comment "Health and beauty."@en ;
    rdfs:subClassOf schema:LocalBusiness .

schema:EntertainmentBusiness a owl:Class ;
    rdfs:label "EntertainmentBusiness"@en ;
    rdfs:comment "A business providing entertainment."@en ;
    rdfs:subClassOf schema:LocalBusiness .

schema:LodgingBusiness a owl:Class ;
    rdfs:label "LodgingBusiness"@en ;
    rdfs:comment "A lodging business, such as a motel, hotel, or inn."@en ;
    rdfs:subClassOf schema:LocalBusiness .

schema:MedicalOrganization a owl:Class ;
    rdfs:label "MedicalOrganization"@en ;
    rdfs:comment "A medical organization (physical or not), such as hospital, institution or clinic."@en ;
    rdfs:subClassOf schema:Organization .

nco:hasElement a owl:ObjectProperty ;
    rdfs:label "has element"@en ;
    rdfs:domain nco:Collection ;
    rdfs:range owl:Thing ;
    owl:inverseOf nco:isElementOf ;
    owl:propertyChainAxiom ( nco:hasUnifyingFactor nco:ufSatisfiedBy ) .

nco:topicRoleOf a owl:FunctionalProperty,
        owl:InverseFunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "is topic role of"@en ;
    rdfs:comment "A given topic role is played by one and only thing."@en ;
    rdfs:domain nco:Topic ;
    rdfs:range owl:Thing .

schema:HomeAndConstructionBusiness a owl:Class ;
    rdfs:label "HomeAndConstructionBusiness"@en ;
    rdfs:comment "A construction business.\\n\\nA HomeAndConstructionBusiness is a [[LocalBusiness]] that provides services around homes and buildings.\\n\\nAs a [[LocalBusiness]] it can be described as a [[provider]] of one or more [[Service]]\\(s)."@en ;
    rdfs:subClassOf schema:LocalBusiness .

nco:AgentComponent a owl:Class ;
    rdfs:label "Agent component"@en ;
    rdfs:seeAlso "nao:AgentComponent" ;
    rdfs:subClassOf _:nd85a7a3bb83a4db0a640b389672634e5b38,
        [ a owl:Restriction ;
            owl:onClass nco:Agent ;
            owl:onProperty nco:hasAgent ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] .

nco:Collection a owl:Class ;
    rdfs:label "Collection"@en ;
    rdfs:comment "Membership of a Collection is predicated on meeting the conditions associated with the associated unifying factor (Carriero et al., 2021)."@en ;
    rdfs:seeAlso "Class Collection in Dolce D0, dul:Collection"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom nco:UnifyingFactor ;
            owl:onProperty nco:hasUnifyingFactor ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass nco:UnifyingFactor ;
            owl:onProperty nco:hasUnifyingFactor ] ;
    owl:disjointUnionOf ( nco:CollectionOfEvents nco:Description nco:Situation nco:Viewpoint ) .

nco:Statement a owl:Class ;
    rdfs:label "Statement"@en ;
    rdfs:comment "A member of this class is an individual that reifies a triple."@en .

schema:AutomotiveBusiness a owl:Class ;
    rdfs:label "AutomotiveBusiness"@en ;
    rdfs:comment "Car repair, sales, or parts."@en ;
    rdfs:subClassOf schema:LocalBusiness .

schema:FoodEstablishment a owl:Class ;
    rdfs:label "FoodEstablishment"@en ;
    rdfs:comment "A food-related business."@en ;
    rdfs:subClassOf schema:LocalBusiness .

schema:SportsActivityLocation a owl:Class ;
    rdfs:label "SportsActivityLocation"@en ;
    rdfs:comment "A sports location, such as a playing field."@en ;
    rdfs:subClassOf schema:LocalBusiness .

nco:Claim a owl:Class ;
    rdfs:label "Claim"@en ;
    rdfs:comment "A claim is logically characterised in this ontology as a reified statement and is also associated with a textual utterance"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom nco:Topic ;
            owl:onProperty nco:concernsTopic ],
        [ a owl:Restriction ;
            owl:allValuesFrom nco:AgentComponent ;
            owl:onProperty nco:hasAgentComponent ],
        [ a owl:Restriction ;
            owl:allValuesFrom nco:Justification ;
            owl:onProperty nco:hasJustification ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass nco:Topic ;
            owl:onProperty nco:concernsTopic ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass nco:AgentComponent ;
            owl:onProperty nco:hasAgentComponent ],
        [ a owl:Restriction ;
            owl:onClass nco:NewsItem ;
            owl:onProperty nco:claimInNewsItem ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass nco:Utterance ;
            owl:onProperty nco:hasUtterance ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        nco:Statement .

nco:NewsItem a owl:Class ;
    rdfs:label "News item"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass nco:NewsSource ;
            owl:onProperty nco:hasNewsSource ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty nco:hasTitle ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:anyURI ;
            owl:onProperty nco:hasURL ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        owl:Thing .

nco:EventOrSituation a owl:Class ;
    rdfs:comment "A pragmatic class to avoid having to refer to the union of Event and Situation in several expressions"@en ;
    owl:equivalentClass [ a owl:Class ;
            owl:unionOf ( nco:Event nco:Situation ) ] .

nco:Topic a owl:Class ;
    rdfs:label "Topic"@en ;
    rdfs:comment "A Topic provides the main focus of a news item. A Topic can be an Entity, an Event (or a Collection of Events), a Viewpoint, a Situation or a Categorical Topic"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty nco:topicRoleOf ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty nco:topicRoleOf ],
        skos:Concept .

nco:Event a owl:Class ;
    rdfs:label "Event"@en ;
    rdfs:comment "The notion of Event in NCO is based on the one used by the News Angle Ontology (Motta et al., 2020), which is in turn based on the Simple Event Model (van Hage et al., 2011).  An event in this model is characterised in terms of time, location and the actors involved in the event.  The relation between events and actors is mediated by actor components, which specify the role played by the actor in question in the event."@en ;
    rdfs:seeAlso "nao:Event" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom nco:AgentComponent ;
            owl:onProperty nco:hasAgentComponent ],
        [ a owl:Restriction ;
            owl:allValuesFrom nco:Location ;
            owl:onProperty nco:hasLocation ],
        nco:EventOrSituation .

schema:Organization a owl:Class ;
    rdfs:label "Organization"@en ;
    rdfs:comment "An organization such as a school, NGO, corporation, club, etc."@en ;
    rdfs:subClassOf schema:Thing ;
    owl:equivalentClass nco:Organization .

schema:MedicalBusiness a owl:Class ;
    rdfs:label "MedicalBusiness"@en ;
    rdfs:comment "A particular physical or virtual business of an organization for medical purposes. Examples of MedicalBusiness include different businesses run by health professionals."@en ;
    rdfs:subClassOf schema:LocalBusiness .

schema:LocalBusiness a owl:Class ;
    rdfs:label "LocalBusiness"@en ;
    rdfs:comment "A particular physical business or branch of an organization. Examples of LocalBusiness include a restaurant, a particular branch of a restaurant chain, a branch of a bank, a medical practice, a club, a bowling alley, etc."@en ;
    rdfs:subClassOf schema:Organization,
        schema:Place .

schema:Store a owl:Class ;
    rdfs:label "Store"@en ;
    rdfs:comment "A retail good store."@en ;
    rdfs:subClassOf schema:LocalBusiness .

[] a owl:Axiom ;
    rdfs:comment "Agent role is expected but we do not make it mandatory, given that in some cases we may simply refer to an agent without specifying the role played in the event or claim" ;
    owl:annotatedProperty rdfs:subClassOf ;
    owl:annotatedSource nco:AgentComponent ;
    owl:annotatedTarget _:nd85a7a3bb83a4db0a640b389672634e5b38 .

[] a owl:AllDisjointClasses ;
    owl:members ( nco:Agent nco:Collection nco:Event nco:NewsItem nco:Statement nco:UnifyingFactor ) .

_:nd85a7a3bb83a4db0a640b389672634e5b38 a owl:Restriction ;
    owl:allValuesFrom nco:AgentRole ;
    owl:onProperty nco:hasAgentRole .

