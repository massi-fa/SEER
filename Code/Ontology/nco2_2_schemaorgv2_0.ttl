nco:Organization a owl:Class ;
    owl:equivalentClass <http://schema.org/Organization> .

nco:hasAgent owl:inverseOf nco:isAgentOf .

nco:hasAgentRole owl:inverseOf nco:isAgentRoleOf .

nco:hasNewsSource owl:inverseOf nco:isNewsSourceOf .

nco:hasPublisher owl:inverseOf nco:isPublisherOf .

nco:hasUtterance owl:inverseOf nco:isUtteranceOf .

nco:hasUtteranceType owl:inverseOf nco:isUtteranceTypeOf .

nco:isAgentOf a owl:ObjectProperty ;
    rdfs:label "is agent of"@en ;
    rdfs:comment "Inverse property of hasAgent. Relates an Agent to the AgentComponents it is part of."@en ;
    rdfs:domain nco:Agent ;
    rdfs:range nco:AgentComponent ;
    owl:inverseOf nco:hasAgent .

nco:isAgentRoleOf a owl:ObjectProperty ;
    rdfs:label "is agent role of"@en ;
    rdfs:comment "Inverse property of hasAgentRole. Relates an AgentRole to the AgentComponents it is assigned to."@en ;
    rdfs:domain nco:AgentRole ;
    rdfs:range nco:AgentComponent ;
    owl:inverseOf nco:hasAgentRole .

nco:isNewsSourceOf a owl:ObjectProperty ;
    rdfs:label "is news source of"@en ;
    rdfs:comment "Inverse property of hasNewsSource. Relates a NewsSource to the NewsItems it is source of."@en ;
    rdfs:domain nco:NewsSource ;
    rdfs:range nco:NewsItem ;
    owl:inverseOf nco:hasNewsSource .

nco:isPublisherOf a owl:ObjectProperty ;
    rdfs:label "is publisher of"@en ;
    rdfs:comment "Inverse property of hasPublisher. Relates a Publisher to the NewsSources it publishes."@en ;
    rdfs:domain nco:Publisher ;
    rdfs:range nco:NewsSource ;
    owl:inverseOf nco:hasPublisher .

nco:isUtteranceOf a owl:ObjectProperty ;
    rdfs:label "is utterance of"@en ;
    rdfs:comment "Inverse property of hasUtterance. Relates an Utterance to the Claim it belongs to."@en ;
    rdfs:domain nco:Utterance ;
    rdfs:range nco:Claim ;
    owl:inverseOf nco:hasUtterance .

nco:isUtteranceTypeOf a owl:ObjectProperty ;
    rdfs:label "is utterance type of"@en ;
    rdfs:comment "Inverse property of hasUtteranceType. Relates an UtteranceType to the Utterances it classifies."@en ;
    rdfs:domain nco:UtteranceType ;
    rdfs:range nco:Utterance ;
    owl:inverseOf nco:hasUtteranceType .

